{
    "type": "array",
    "id": "urn:jsonschema:ru:sbrf:efs:platform:storage:model:Application",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
            "id": {
                "type": "object",
                "id": "urn:jsonschema:ru:sbrf:ufs:platform:storage:impl:id:UUIDEntityId",
                "properties": {
                    "id": {
                        "type": "string"
                    }
                }
            },
            "name": {
                "type": "string",
                "maxLength": 117
            },
            "title": {
                "type": "string",
                "maxLength": 512
            },
            "channelName": {
                "type": "string",
                "maxLength": 256
            },
            "url": {
                "type": "string",
                "maxLength": 256
            },
            "applicationIcon": {
                "type": "string",
                "maxLength": 128
            },
            "order": {
                "type": "integer"
            },
            "deviceType": {
                "type": "string",
                "enum": [
                         "MOBILE",
                         "WEB"
                         ]
            },
            "appId": {
                "type": "string",
                "maxLength": 36
            },
            "version": {
                "type": "string",
                "maxLength": 128
            },
            "settings": {
                "type": "string",
                "maxLength": 1024
            },
            "default": {
                "type": "boolean"
            }
        }
    },
    "minItems": 1,
    "additionalItems": false
}
